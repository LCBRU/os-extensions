<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
  
  <bean id="stgImporterLiquibase" class="liquibase.integration.spring.SpringLiquibase">
    <property name="dataSource" ref="dataSource"/>
    <property name="changeLog" value="classpath:wcmc/db/changelog-master.xml"/>
  </bean>
  
  <bean id="stagingImporterInterfaceHandler" class="com.krishagni.openspecimen.staging.importer.InterfaceHandler" abstract="true">
    <property name="daoFactory" ref="biospecimenDaoFactory"/>
  </bean>
  
  <bean id="stagingParticipantImporterTask" class="com.krishagni.openspecimen.staging.importer.job.StagingParticipantImporterTask">
    <property name="importer" ref="stagingParticipantImporter"/>
  </bean>
  
  <bean id="stagingParticipantImporter" class="com.krishagni.openspecimen.staging.importer.job.StagingParticipantImporter">
    <property name="participantSvc" ref="stagedParticipantSvc"/>
  </bean>
  
  <bean id="stgImpJobUtil" class="com.krishagni.openspecimen.staging.importer.util.StgImpJobUtil">
    <property name="daoFactory" ref="biospecimenDaoFactory"/>
  </bean>
</beans>
